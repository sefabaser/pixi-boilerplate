    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Main Process",
      "address": "localhost",
      "port": 5858,
      "localRoot": "${workspaceRoot}",
      "remoteRoot": "${workspaceRoot}"
    }
  ]
}
